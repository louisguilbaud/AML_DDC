<!DOCTYPE html>
<html>
<head>
    <title>API Call Example</title>
    <style>
        body {
            font-family: Arial, sans-serif;
        }
        .container {
            max-width: 800px;
            margin: 0 auto;
            padding: 20px;
        }
    </style>
</head>
<body>

<div class="container" id="resultContainer">
    <h2>API Result</h2>
    <div id="apiResult"></div>
</div>

<script>
    // Make API call and display result
    fetch('https://sasserver.demo.sas.com/svi-datahub/documents/alerts/146319a9-d567-421b-b246-3a1b9cdcdc55/attachments')
        .then(response => {
            if (!response.ok) {
                throw new Error('Network response was not ok');
            }
            return response.json();
        })
        .then(data => {
            // Display API result
            displayResult(data);
        })
        .catch(error => {
            console.error('There was a problem with the fetch operation:', error);
            document.getElementById('apiResult').textContent = 'Error fetching data from the API';
        });

    // Function to display API result
    function displayResult(data) {
        // Assuming the API returns JSON data, you can format and display it here
        var resultHtml = '<pre>' + JSON.stringify(data, null, 2) + '</pre>';
        document.getElementById('apiResult').innerHTML = resultHtml;
    }
</script>

</body>
</html>
